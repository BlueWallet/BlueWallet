  "scripts": {
    "clean": "cd android/; ./gradlew clean; cd ..; rm -r -f /tmp/metro-cache/; rm -r -f node_modules/; yarn cache clean --force; yarn install",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "prepare:prod": "cp ./config/prod/google-services.json ./android/app && cp ./config/prod/GoogleService-Info.plist ./ios && cp ./config/prod/.env ./ && cp ./config/prod/app.json ./ && cp ./config/prod/MainActivity.java ./android/app/src/main/java/io/goldwallet/wallet",
    "prepare:beta": "cp ./config/beta/google-services.json ./android/app && cp ./config/beta/GoogleService-Info.plist ./ios && cp ./config/beta/.env ./ && cp ./config/beta/app.json ./ && cp ./config/beta/MainActivity.java ./android/app/src/main/java/io/goldwallet/wallet",
    "android:prod": "yarn prepare:prod && react-native run-android --variant=prodDebug",
    "android:beta": "yarn prepare:beta && react-native run-android --variant=betaDebug",
    "ios:prod": "yarn prepare:prod && react-native run-ios",
    "ios:beta": "yarn prepare:beta && react-native run-ios --scheme='GoldWallet beta'",
    "postinstall": "./node_modules/.bin/rn-nodeify --install buffer,events,process,stream,util,inherits,fs,path --hack; npx jetify",
    "ios": "react-native run-ios",
    "test": "CI=true yarn jest",
    "test:local": "yarn jest",
    "unit": "echo",
    "jest": "node node_modules/jest/bin/jest.js tests/*",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "typescript:check": "tsc --noEmit",
    "prettier:fix": "prettier --write '**/*.{js,ts,tsx}'",
    "precommit": "yarn lint-staged",
    "shake": "adb shell input keyevent 82",
    "translate:check-missing": "node --experimental-modules -r @std/esm scripts/checkTranslation.mjs"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "setupFiles": [
