{
  "parser": "babel-eslint",
  "plugins": [
      "react", "prettier"
  ],
  "extends": ["standard", "standard-react", "prettier"],
  "rules": {
    'prettier/prettier': [
      'warn',
      {
        singleQuote: true,
        printWidth: 140,
        trailingComma: 'all'
      }
    ]
  },
  "env":{
    "es6": true
  },
  "globals": { "fetch": false }
}
