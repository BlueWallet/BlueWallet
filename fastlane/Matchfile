git_url(ENV["GIT_URL"])

storage_mode("git")

type("appstore") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier(["io.bluewallet.bluewallet"])
username(ENV["APPLE_ID"]) # Your Apple Developer Portal username

